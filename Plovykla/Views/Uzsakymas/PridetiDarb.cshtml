@model Plovykla.Models.Uzsakymas

@{
    ViewData["Title"] = "PridetiDarb";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h3 class="login">Darbuotojo pridėjimas</h3>
<div class="row">
    <div class="col-md-4">
        <form asp-action="PridetiDarbAct">
            <div class="form-group">
                <label>Darbuotojas</label>
                <select asp-for="darbuotojoId" class="form-control" asp-items="ViewBag.darbuotojai"></select>
            </div>
            <input type ="hidden" name="uzsakymoId" id="uzsakymoId" value="@ViewBag.id"/>
            <input type ="submit" name="submit" value="Pridėti darbuotoją"/>
        </form>
    </div>
</div>



